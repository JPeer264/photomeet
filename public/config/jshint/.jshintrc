		"_V_": false, // jasmine
		"afterEach": false, // jasmine
		"beforeEach": false, // jasmine
		"confirm": false, // jasmine
		"context": false, // jasmine
		"jasmine": false, // jasmine
		"JSHINT": false, // jasmine
		"mostRecentAjaxRequest": false, // jasmine
		"qq": false, // jasmine
		"runs": false, // jasmine
		"spyOn": false, // jasmine
		"spyOnEvent": false, // jasmine
		"waitsFor": false, // jasmine
		"xdescribe": false, // jasmine
		"navigator": false, // HTML 5,
		"Placeholders": false, // Placeholders
		"emb": false, // Daimler EMB Messagebus
		"ria": false, // Daimler RIA integration framework
		"MB_DealerDisplay_Widget": false, // Dealer Display global
